{
    "env": {
        "browser": true,
        "es2021": true,
        "vue/setup-compiler-macros": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
        "no-useless-escape": "off",
        "vue/multi-word-component-names": "off",
        "vue/no-multiple-template-root": "off",
        "no-prototype-builtins": "off"
    }
}
